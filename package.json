  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "jquery": "^2.1.3",
    "node-hipchat": "^0.4.5",
    "randomstring": "^1.0.3",
    "space-pen": "^5.0.1",
    "underscore": "^1.7.0"
  }
}
